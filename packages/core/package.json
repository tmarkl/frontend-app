{
  "name": "@frontend/core",
  "version": "0.1.0",
  "description": "FrontendApp Core",
  "keywords": [
    "easyapp",
    "redux",
    "saga"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tmarkl/frontend-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tmarkl/frontend-app.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/tmarkl/frontend-app/issues"
  },
  "author": "TianLiang",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "lint": "eslint src",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "NODE_ENV=development rollup -c -o dist/frontend-core.js",
    "build:umd:min": "NODE_ENV=production rollup -c -o dist/frontend-core.min.js",
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min",
    "prebuild": "yarn clean",
    "prepare": "yarn lint && yarn build"
  },
  "dependencies": {
    "query-string": "6.11.0",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "rollup-plugin-terser": "^5.2.0",
    "typescript": "^3.8.2"
  }
}
